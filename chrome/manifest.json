{
  "name": "Rip Quick Add",
  "description": "Quickly create Google calendar events from natural language.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "contextMenus"
  ],
  "host_permissions": [
    "https://timkovi.ch/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
